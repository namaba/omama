

# Ignore bundler config.
/backend/.bundle
/backend/vendor/bundle

# # Ignore all logfiles and tempfiles.
/backend/log/*
/backend/tmp/*
!/backend/log/.keep
!/backend/tmp/.keep

# Ignore pidfiles, but keep the directory.
/backend/tmp/pids/*
!/backend/tmp/pids/
!/backend/tmp/pids/.keep

# Ignore uploaded files in development.
/backend/storage/*
!/backend/storage/.keep

/backend/public/assets
/backend/.byebug_history

# Ignore master key for decrypting credentials and more.
/backend/config/master.key

/backend/public/packs
/backend/public/packs-test
/backend/node_modules
/backend/yarn-error.log
/backend/yarn-debug.log*
/backend/.yarn-integrity
/backend/.env

# front
# See https://help.github.com/articles/ignoring-files/ for more about ignoring files.

# dependencies
/frontend/app/node_modules
/frontend/app/.pnp
/frontend/app.pnp.js

# testing
/frontend/app/coverage

# next.js
/frontend/app/.next/
/frontend/app/out/

# production
/frontend/app/build

# misc
/frontend/app/.DS_Store
/frontend/app/*.pem

# debug
/frontend/app/npm-debug.log*
/frontend/app/yarn-debug.log*
/frontend/app/yarn-error.log*

# local env files
/frontend/app/.env.local
/frontend/app/.env.development.local
/frontend/app/.env.test.local
/frontend/app/.env.production.local

# vercel
/frontend/app/.vercel
